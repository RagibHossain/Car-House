<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Car House</title>

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/custom.css" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Lato&display=swap" rel="stylesheet">
    
 


    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


</head>
<body>

    <section class="navsec">
        <a class="navbar-brand" href="#"><img class="logo" src="~/images/logo.png"></a>
        <nav class="navbar navbar-expand-xl navbar-light bg-light">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a asp-controller="Home" asp-action="Index" class="nav-link">HOME<span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="about">ABOUT US</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" asp-controller="Home" asp-action="product" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            PRODUCTS
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" asp-controller="Product" asp-action="ProductCategory" asp-route-selectedCategory="Sedan">Sedan</a>
                            <a class="dropdown-item" asp-controller="Product" asp-action="ProductCategory" asp-route-selectedCategory="Suv">Suv</a>
                            <a class="dropdown-item" asp-controller="Product" asp-action="ProductCategory" asp-route-selectedCategory="Sport">Sport</a>
                            <a class="dropdown-item" asp-controller="Product" asp-action="ProductCategory" asp-route-selectedCategory="Minivan">Minivan</a>
                            <div class="dropdown-divider"></div>
                            <a asp-controller="Product" asp-action="Products" class="dropdown-item">See All</a>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="service">SERVICES </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="drvn">DRVN</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="contact">CONTACT</a>
                    </li>
                </ul>
                <form class="form-inline">
                    <input class="" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-success my-2 my-sm-0" type="submit"><i class="fa fa-search"></i></button>
                </form>
            </div>
        </nav>
    </section>

    <div class="">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <section class="footersec">
        <!-- Footer -->
        <footer class="page-footer font-small stylish-color-dark pt-4">

            <!-- Footer Links -->
            <div class="container text-center text-md-left">

                <!-- Grid row -->
                <div class="row">

                    <!-- Grid column -->
                    <div class="col-md-4 mx-auto">

                        <!-- Content -->
                        <a href="#"><img class="footerlogo" src="~/images/logo.png"></a>
                        <p>
                            Car House Limited inaugurated in 1991 and has been importing brand new and reconditioned
                            vehicles for the last two decades.
                        </p>

                    </div>
                    <!-- Grid column -->

                    <hr class="clearfix w-100 d-md-none">

                    <!-- Grid column -->
                    <div class="list col-md-2 mx-auto">

                        <!-- Links -->
                        <h5 class="text-uppercase mt-3 mb-4">SERVICES</h5>

                        <ul class="list-unstyled">
                            <li>
                                <a href="#!">Brand new cars.</a>
                            </li>
                            <li>
                                <a href="#!">Reconditioned cars</a>
                            </li>
                            <li>
                                <a href="#!">Customize to your will</a>
                            </li>
                            <li>
                                <a href="#!">Import the best cars</a>
                            </li>
                        </ul>

                    </div>
                    <!-- Grid column -->

                    <hr class="clearfix w-100 d-md-none">

                    <!-- Grid column -->
                    <div class="list col-md-2 mx-auto">

                        <!-- Links -->
                        <h5 class="text-uppercase mt-3 mb-4">CUSTOMERS</h5>

                        <ul class="list-unstyled">
                            <li>
                                <a href="#!">Recent sales</a>
                            </li>
                            <li>
                                <a href="#!">Reviews</a>
                            </li>
                            <li>
                                <a href="#!">Videos</a>
                            </li>
                            <li>
                                <a href="#!">Share your story</a>
                            </li>
                        </ul>

                    </div>
                    <!-- Grid column -->

                    <hr class="clearfix w-100 d-md-none">

                    <!-- Grid column -->
                    <div class="list col-md-2 mx-auto">

                        <!-- Links -->
                        <h5 class="text-uppercase mt-3 mb-4">CONTACT</h5>

                        <ul class=" contact list-unstyled">
                            <li>
                                <i class="fa fa-mobile" aria-hidden="true"></i>+8801234567890
                            </li>
                            <li>
                                <i class="fa fa-phone" aria-hidden="true"></i>+88020123456
                            </li>
                            <li>
                                <i class="fa fa-envelope" aria-hidden="true"></i>Carhouse@sth.com
                            </li>
                            <li>
                                <i class="fa fa-map-marker" aria-hidden="true"></i><a href="#!">Showroom Map</a>
                            </li>
                        </ul>

                    </div>
                    <!-- Grid column -->

                </div>
                <!-- Grid row -->

            </div>
            <!-- Footer Links -->

            <hr>

            <hr>

            <!-- Social buttons -->
            <ul class=" social list-unstyled list-inline text-center">
                <li class="list-inline-item">
                    <a class="btn-floating btn-fb mx-1">
                        <a href="#" class="fa fa-facebook"></a>
                    </a>
                </li>
                <li class="list-inline-item">
                    <a class="btn-floating btn-tw mx-1">
                        <a href="#" class="fa fa-twitter"></a>
                    </a>
                </li>
                <li class="list-inline-item">
                    <a class="btn-floating btn-gplus mx-1">
                        <a href="#" class="fa fa-google"></a>
                    </a>
                </li>
                <li class="list-inline-item">
                    <a class="btn-floating btn-li mx-1">
                        <a href="#" class="fa fa-youtube"></a>
                    </a>
                </li>
            </ul>
            <!-- Social buttons -->
            <!-- Copyright -->
            <div class="footer-copyright text-center py-3">
                © 2020 Copyright:
                <i>CAR HOUSE</i>
            </div>
            <!-- Copyright -->

        </footer>
        <!-- Footer -->
    </section>


    <script src="js/wow.min.js"></script>
    <script>
        new WOW().init();
    </script>

    <script src="~/js/rellax.min.js"></script>
    <script type="text/javascript">var rellax = new Rellax('.rellax');</script>
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>






</body>
</html>
